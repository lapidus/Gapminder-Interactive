package org.gapminder {	import com.greensock.TweenMax;
	
	import mx.core.IUIComponent;
		public class QuestionPair_OLD extends QuestionPair_design {				public function QuestionPair_OLD(alt1_text:String, alt2_text:String) {						this.result1.bar.visible = false;			this.result2.bar.visible = false;						this.result1.percent.visible = false;			this.result2.percent.visible = false;						this.alt1.text = alt1_text;			this.alt2.text = alt2_text;		}				public function showResult(percentageAlt1:Number) {					this.result1.bar.visible = true;			this.result2.bar.visible = true;			TweenMax.to(result1.bar,0.2,{width: percentageAlt1*100});			TweenMax.to(result2.bar,0.2,{width: (1-percentageAlt1)*100, x: -((1-percentageAlt1)*100)});															/*			_questionPairs[_activeQuestion].alt1percent.text = (percentageAlt1*100).toString() + "%";			_questionPairs[_activeQuestion].alt2percent.text = ((1-percentageAlt1)*100).toString() + "%";						_questionPairs[_activeQuestion].alt1percent.visible = true;			_questionPairs[_activeQuestion].alt2percent.visible = true;		*/		}			}	}